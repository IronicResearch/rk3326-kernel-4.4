&pwm0 {
	compatible = "rockchip,remotectl-pwm";
	interrupts = <GIC_SPI 24 IRQ_TYPE_LEVEL_HIGH>;
	pinctrl-names = "default";
	pinctrl-0 = <&pwm0_pin>;
	remote_support_psci = <1>;
	remote_pwm_id = <0>;
	handle_cpu_id = <1>;
	status = "okay";
